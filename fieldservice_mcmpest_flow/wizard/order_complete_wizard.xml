<odoo>
    <record id="view_order_complete_wizard" model="ir.ui.view">
        <field name="name">Add Resolution notes to FSM Order</field>
        <field name="model">order.complete.wizard</field>
        <field name="arch" type="xml">
            <form string="Complete FSM Order">
                Enter notes about the order completion here.
                <group>
                    <field name="fsm_order_id" invisible="1"/>
                    <field name="resolution"/>
                </group>
                <footer>
                    <button string="Finish" type="object" name="action_finish_wizard" class="btn-primary"/>
                    <button string="Cancel" special="cancel" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
